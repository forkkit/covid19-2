{
    "bodyTag": "<body>",
    "mainInput": "./src/Main.mjs",
    "mainOutput": "main.js",
    "workers": {
        "data": {
            "input": "./src/worker/Data.mjs",
            "output": "dataworker.js"
        },
        "vdom": {
            "input": "./src/worker/VDom.mjs",
            "output": "vdomworker.js"
        }
    },
    "apps": {
        "covid19": {
            "input": "buildScripts/entrypoints/covid19.mjs",
            "output": "/apps/covid19/",
            "themes": "'neo-theme-dark'",
            "title": "covid19"
        },
        "Docs": {
            "input": "buildScripts/entrypoints/Docs.mjs",
            "mainThreadAddons": "'GoogleAnalytics', 'HighlightJS', 'Stylesheet'",
            "output": "/docs/",
            "title": "Neo Docs"
        }
    }
}
